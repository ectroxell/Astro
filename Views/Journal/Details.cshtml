@using Humanizer
@model Journal


<div class="jumbotron bg-pink-clouds">
    <h3 class="text-white">Moon Journal from @Model.Date.Humanize()</h3>
    <hr class="hr-white" />
    <p class="lead text-white">@Model.MoonPhase.Name</p>
    <p class="text-white">@Model.MoonPhase.Keywords: @Model.MoonPhase.DescriptionShort</p>
</div>

<div>
    @Model.JournalText
</div>